---
import LoginLayout from "../../layouts/LoginLayout";

export function getStaticPaths() {
    return [{ params: { lang: "pl-en" } }];
}

const { lang } = Astro.params;
---

<html>
    <head></head>
    <body>
        <LoginLayout client:load />
    </body>
</html>

<style>
    html {
        @apply h-full;
    }
    body {
        @apply h-full;
    }
</style>
